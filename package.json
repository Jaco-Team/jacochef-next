{
  "name": "jacochef-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && npm run sentry:sourcemaps",
    "start": "next start",
    "lint": "next lint",
    "deploy:local": "next build && pm2 startOrRestart ecosystem.config.js --env local",
    "deploy:dev": "next build && pm2 startOrRestart ecosystem.config.js --env development",
    "deploy:prod": "npm update && next build && npm run sentry:sourcemaps && pm2 startOrRestart ecosystem.config.js --env production",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org sentry --project jacosoft-dop-react ./out && sentry-cli --url https://sentry.jacochef.ru/ sourcemaps upload --org sentry --project jacosoft-dop-react ./out"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^5.17.1",
    "@mui/lab": "^5.0.0-alpha.175",
    "@mui/material": "^5.17.1",
    "@mui/material-nextjs": "^5.16.14",
    "@mui/styled-engine-sc": "^5.14.12",
    "@mui/x-date-pickers": "^7.28.3",
    "@sentry/cli": "^2.43.0",
    "@sentry/react": "^9.13.0",
    "@tinymce/tinymce-react": "^5.1.1",
    "axios": "^1.8.4",
    "childprocess": "^2.0.2",
    "common-es": "^1.0.1",
    "dayjs": "^1.11.13",
    "dropzone": "^6.0.0-beta.2",
    "js-cookie": "^3.0.5",
    "mathjs": "^14.4.0",
    "moment": "^2.30.1",
    "next": "14.2.28",
    "node-tls": "^0.10.2",
    "query-string": "^9.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.4.6",
    "react-modal-image": "^2.6.0",
    "react-multi-date-picker": "^4.5.2",
    "sass": "^1.86.3",
    "styled-components": "^5.3.11",
    "tinymce": "^7.8.0",
    "zustand": "^4.5.6"
  }
}