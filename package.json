{
  "name": "jacochef-next",
  "version": "0.1.0",
  "private": true,
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md,yml,yaml,css,scss,html,mjs,cjs}": "prettier --write"
  },
  "scripts": {
    "postinstall": "mkdir -p public/tinymce/skins/ui/oxide && cp -r node_modules/tinymce/skins/ui/oxide/* public/tinymce/skins/ui/oxide/",
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "prepare": "husky",
    "deploy:local": "next build && pm2 startOrRestart ecosystem.config.js --env local",
    "deploy:dev": "next build && pm2 startOrRestart ecosystem.config.js --env development",
    "deploy_test:prod": "npm update && next build && npm run sentry:sourcemaps && pm2 startOrRestart ecosystem.config.js --env production",
    "deploy:prod": "npm update && next build && pm2 startOrRestart ecosystem.config.js --env production",
    "sentry:sourcemaps": "sentry-cli --url https://sentry.jacochef.ru/ sourcemaps upload --org sentry --project jacosoft-dop-react ./.next --rewrite"
  },
  "dependencies": {
    "@amcharts/amcharts5": "^5.14.3",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@hello-pangea/dnd": "^18.0.1",
    "@mui/icons-material": "^7.3.4",
    "@mui/lab": "^7.0.1-beta.18",
    "@mui/material": "^7.3.4",
    "@mui/x-date-pickers": "^8.15.0",
    "@sentry/cli": "^2.57.0",
    "@sentry/react": "^10.22.0",
    "@tinymce/tinymce-react": "^6.3.0",
    "axios": "^1.13.1",
    "childprocess": "^2.0.2",
    "common-es": "^1.1.0",
    "cookie": "^1.0.2",
    "dayjs": "^1.11.18",
    "dropzone": "^6.0.0-beta.2",
    "file-saver": "^2.0.5",
    "js-cookie": "^3.0.5",
    "mathjs": "^15.0.0",
    "moment": "^2.30.1",
    "next": "16.0.1",
    "node-tls": "^0.10.2",
    "query-string": "^9.3.1",
    "react": "19.2.0",
    "react-dev-utils": "^12.0.1",
    "react-dom": "19.2.0",
    "react-draggable": "^4.5.0",
    "react-modal-image": "^2.6.0",
    "react-multi-date-picker": "^4.5.2",
    "tinymce": "^7.9.1",
    "tr46": "^5.1.1",
    "xlsx": "https://cdn.sheetjs.com/xlsx-latest/xlsx-latest.tgz",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@types/dropzone": "^5.7.9",
    "@types/file-saver": "^2.0.7",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "sass": "^1.93.2"
  }
}
